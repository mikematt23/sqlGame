const express = require('express')
const userActions = require('../models/usermodels')

const router = express.Router()

router.get('/',(req,res)=>{
  res.render('home')
})

router.get('/logIn',(req,res)=>{
  res.render('log-in')
})

router.get('/signUp',(req,res)=>{
  res.render('sign-up')
})

router.get('/user',(req,res)=>{
  res.render('user')
})

module.exports = router